# Uncomment this line to define a global platform for your project
 platform :ios, '9.0'

target 'DemoLoginsSDKs' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DemoLoginsSDKs
pod 'Google/SignIn'
  pod 'Google/Analytics'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'LinkedinSwift', '~> 1.6.2'
  pod 'Fabric'
  pod 'TwitterKit'
  pod 'TwitterCore'


  target 'DemoLoginsSDKsTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Google/SignIn'
  pod 'Google/Analytics'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'LinkedinSwift', '~> 1.6.2'
  pod 'Fabric'
  pod 'TwitterKit'
  pod 'TwitterCore'

  end

  target 'DemoLoginsSDKsUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'Google/SignIn'
  pod 'Google/Analytics'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'LinkedinSwift', '~> 1.6.2'
  pod 'Fabric'
  pod 'TwitterKit'
  pod 'TwitterCore'

  end

end
